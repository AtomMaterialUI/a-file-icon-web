{
  "name": "atom-material-icons-web",
  "version": "7.0.2",
  "description": "Atom Material Icons for GitHub",
  "author": "Elior Boukhobza <heliosaian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.4",
    "js-yaml": "4.1.0",
    "plasmo": "0.58.0",
    "polka": "0.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "select-dom": "7.1.1",
    "sirv": "2.0.2",
    "sirv-cli": "2.0.2",
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5"
  },
  "scripts": {
    "clean": "rimraf public",
    "icons": "gulp icons",
    "folders": "gulp folders",
    "convert": "npm --prefix iconGenerator run convert",
    "build": "npm run clean && npm run convert && npm run public && npm run plasmo:build",
    "autobuild": "rollup -c -w",
    "public": "gulp prepare",
    "dev": "run-p start:dev autobuild",
    "prestart:dev": "gulp prepare",
    "plasmo:dev": "plasmo dev",
    "plasmo:build": "plasmo build --zip",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "release": "gulp release",
    "webext": "web-ext build --overwrite-dest",
    "webext:test": "web-ext lint",
    "firefox": "npm run build && cp firefox/manifest.json . && npm run webext",
    "chrome": "npm run build && cp chrome/manifest.json . && npm run release"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/runtime": "7.18.9",
    "@plasmohq/prettier-plugin-sort-imports": "3.5.4",
    "@plasmohq/storage": "^0.12.2",
    "@types/chrome": "0.0.200",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-clean-css": "4.3.0",
    "gulp-consolidate": "0.2.0",
    "gulp-copy": "4.0.1",
    "gulp-iconfont": "11.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-zip": "5.1.0",
    "node-fetch": "3.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.4",
    "underscore": "1.13.6",
    "web-ext": "7.3.1"
  },
  "manifest": {
    "name": "Atom Material Icons",
    "description": "Atom Material Icons for GitHub, BitBucket, Gitee and GitLab",
    "author": "Elior Boukhobza (Mallowigi)",
    "homepage_url": "https://www.material-theme.com",
    "host_permissions": [
      "http://github.com/*",
      "https://github.com/*",
      "http://bitbucket.org/*",
      "https://bitbucket.org/*",
      "http://gitlab.com/*",
      "https://gitlab.com/*",
      "http://gitee.com/*",
      "https://gitee.com/*"
    ]
  },
  "peerDependencies": {
    "xml2json-cli": "1.1.1"
  },
  "optionalDependencies": {
    "lodash.template": "4.5.0",
    "minimist": "1.2.6"
  }
}
