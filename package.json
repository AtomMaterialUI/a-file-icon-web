{
  "name": "atom-material-icons-web",
  "version": "12.0.0",
  "description": "Atom Material Icons for Web",
  "author": "Elior Boukhobza <heliosaian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.4",
    "js-yaml": "4.1.0",
    "plasmo": "0.58.0",
    "polka": "0.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "select-dom": "7.1.1",
    "sirv": "2.0.2",
    "sirv-cli": "2.0.2",
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5"
  },
  "scripts": {
    "preconvert": "rimraf public",
    "convert": "npm --prefix iconGenerator run convert",
    "postconvert": "gulp prepare",
    "prebuild": "npm run convert",
    "build": "npm run plasmo:build",
    "predev": "npm run convert",
    "dev": "npm run plasmo:dev",
    "plasmo:dev": "plasmo dev",
    "plasmo:build": "plasmo build --zip",
    "release": "gulp release",
    "webext": "web-ext build --overwrite-dest",
    "prefirefox": "npm run build && cp firefox/manifest.json .",
    "firefox": "npm run webext",
    "prechrome": "npm run build && cp chrome/manifest.json .",
    "chrome": "npm run release"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.7",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@plasmohq/storage": "^0.13.1",
    "@types/chrome": "0.0.206",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-clean-css": "4.3.0",
    "gulp-consolidate": "0.2.0",
    "gulp-copy": "4.0.1",
    "gulp-iconfont": "11.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-zip": "5.1.0",
    "node-fetch": "3.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "underscore": "1.13.6",
    "web-ext": "7.4.0"
  },
  "manifest": {
    "name": "Atom Material Icons",
    "description": "Atom Material Icons for Web (GitHub, BitBucket, Gitee, Azure and GitLab)",
    "author": "Elior Boukhobza (Mallowigi)",
    "homepage_url": "https://www.material-theme.com",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "http://github.com/*",
      "https://github.com/*",
      "http://bitbucket.org/*",
      "https://bitbucket.org/*",
      "http://gitlab.com/*",
      "https://gitlab.com/*",
      "http://gitee.com/*",
      "https://gitee.com/*",
      "http://dev.azure.com/*",
      "https://dev.azure.com/*"
    ]
  },
  "peerDependencies": {
    "xml2json-cli": "1.1.1"
  },
  "optionalDependencies": {
    "lodash.template": "4.5.0",
    "minimist": "1.2.7"
  }
}
